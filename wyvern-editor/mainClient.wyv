require java
require stdout

import fileSystem
import logger
import serviceModule.reportService

val fs = fileSystem(java)
val logFile = fs.fileFor("log.txt")
val logger = logger(logFile)

logger.updateLog("Started executing main client.\n")
val reportService= reportService(fs, logger)
reportService.submitReport("Report title", "This my first report!!")

stdout.print("Reading Report\n")
stdout.print(reportService.getReport())


